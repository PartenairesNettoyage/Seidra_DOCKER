# Variables pour l'exécution conteneurisée de SEIDRA Ultimate
SEIDRA_ENV=production
SEIDRA_DEBUG=false
SEIDRA_ALLOW_SYSTEM_FALLBACK=false
SEIDRA_DATABASE_URL=postgresql+psycopg://seidra:seidra@postgres:5432/seidra
SEIDRA_ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080
SEIDRA_REDIS_URL=redis://redis:6379/0
SEIDRA_CELERY_BROKER=redis://redis:6379/1
SEIDRA_CELERY_BACKEND=redis://redis:6379/2
SEIDRA_RATE_LIMIT_REDIS_URL=redis://redis:6379/3
SEIDRA_MINIO_ENDPOINT=http://minio:9000
SEIDRA_MINIO_ACCESS_KEY=admin
SEIDRA_MINIO_SECRET_KEY=password
SEIDRA_MINIO_BUCKET=seidra-media
SEIDRA_COMFYUI_URL=http://localhost:8188
SEIDRA_SADTALKER_URL=http://localhost:8002
SEIDRA_WS_TOKEN=ultimate-demo-token
# Notifications externes (désactivées par défaut)
SEIDRA_NOTIFICATIONS_SLACK__ENABLED=false
SEIDRA_NOTIFICATIONS_SLACK__WEBHOOK_URL=
SEIDRA_NOTIFICATIONS_SLACK__LEVELS=error,critical
SEIDRA_NOTIFICATIONS_SLACK__USERNAME=SEIDRA Ultimate
SEIDRA_NOTIFICATIONS_SLACK__ICON_EMOJI=:robot_face:
SEIDRA_NOTIFICATIONS_PAGERDUTY__ENABLED=false
SEIDRA_NOTIFICATIONS_PAGERDUTY__ROUTING_KEY=
SEIDRA_NOTIFICATIONS_PAGERDUTY__LEVELS=error,critical
SEIDRA_NOTIFICATIONS_PAGERDUTY__SOURCE=seidra-backend
SEIDRA_NOTIFICATIONS_PAGERDUTY__CLIENT=SEIDRA Ultimate
SEIDRA_NOTIFICATIONS_PAGERDUTY__DEDUP_KEY_PREFIX=seidra-
